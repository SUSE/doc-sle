#. (itstool) path: sect1/title
#: xml/xm_vs_xl_xen2libvirt.xml:9
msgid "Import Xen Domain Configuration into <systemitem class=\"library\">libvirt</systemitem>"
msgstr ""

#. (itstool) path: sect1/para
#: xml/xm_vs_xl_xen2libvirt.xml:17
msgid "<command>xen2libvirt</command> is a command line tool to import legacy Xen domain configuration into the <systemitem class=\"library\">libvirt</systemitem> virtualization library (see The Virtualization book for more information on <systemitem class=\"library\">libvirt</systemitem>). xen2libvirt provides an easy way to import domains managed by the deprecated <command>xm</command>/xend tool stack into the new <systemitem class=\"library\">libvirt</systemitem>/libxl tool stack. Several domains can be imported at once using its <option>--recursive mode</option>"
msgstr ""

#. (itstool) path: sect1/para
#: xml/xm_vs_xl_xen2libvirt.xml:27
msgid "<command>xen2libvirt</command> is included in the <systemitem>xen-tools</systemitem> package. If needed, install it with"
msgstr ""

#. (itstool) path: sect1/screen
#: xml/xm_vs_xl_xen2libvirt.xml:32
#, no-wrap
msgid ""
"zypper install xen-tools"
msgstr ""

#. (itstool) path: sect1/para
#: xml/xm_vs_xl_xen2libvirt.xml:34
msgid "<command>xen2libvirt</command> general syntax is"
msgstr ""

#. (itstool) path: sect1/screen
#: xml/xm_vs_xl_xen2libvirt.xml:38
#, no-wrap
msgid ""
"xen2libvirt &lt;options&gt; <replaceable>/path/to/domain/config</replaceable>"
msgstr ""

#. (itstool) path: sect1/para
#: xml/xm_vs_xl_xen2libvirt.xml:40
msgid "where <option>options</option> can be:"
msgstr ""

#. (itstool) path: varlistentry/term
#: xml/xm_vs_xl_xen2libvirt.xml:46
msgid "<option>-h</option>, <option>--help</option>"
msgstr ""

#. (itstool) path: listitem/para
#: xml/xm_vs_xl_xen2libvirt.xml:49
msgid "Prints short information about <command>xen2libvirt</command> usage."
msgstr ""

#. (itstool) path: varlistentry/term
#: xml/xm_vs_xl_xen2libvirt.xml:55
msgid "<option>-c</option>, <option>--convert-only</option>"
msgstr ""

#. (itstool) path: listitem/para
#: xml/xm_vs_xl_xen2libvirt.xml:58
msgid "Converts the domain configuration to the <systemitem class=\"library\">libvirt</systemitem> XML format, but does not do the import to <systemitem class=\"library\">libvirt</systemitem>."
msgstr ""

#. (itstool) path: varlistentry/term
#: xml/xm_vs_xl_xen2libvirt.xml:65
msgid "<option>-r</option>, <option>--recursive</option>"
msgstr ""

#. (itstool) path: listitem/para
#: xml/xm_vs_xl_xen2libvirt.xml:68
msgid "Converts and/or imports all domains configuration recursively, starting at the specified path."
msgstr ""

#. (itstool) path: varlistentry/term
#: xml/xm_vs_xl_xen2libvirt.xml:75
msgid "<option>-f</option>, <option>--format</option>"
msgstr ""

#. (itstool) path: listitem/para
#: xml/xm_vs_xl_xen2libvirt.xml:78
msgid "Specifies the format of the source domain configuration. Can be either <literal>xm</literal>, or <literal>sexpr</literal> (S-expression format)."
msgstr ""

#. (itstool) path: varlistentry/term
#: xml/xm_vs_xl_xen2libvirt.xml:86
msgid "<option>-v</option>, <option>--verbose</option>"
msgstr ""

#. (itstool) path: listitem/para
#: xml/xm_vs_xl_xen2libvirt.xml:89
msgid "Prints more detailed information about the import process."
msgstr ""

#. (itstool) path: example/title
#: xml/xm_vs_xl_xen2libvirt.xml:97
msgid "Converting Xen Domain Configuration to <systemitem class=\"library\">libvirt</systemitem>"
msgstr ""

#. (itstool) path: example/para
#: xml/xm_vs_xl_xen2libvirt.xml:98
msgid "Suppose you have a Xen domain managed with <command>xm</command> with the following configuration saved in <filename>/etc/xen/sle12.xm</filename>:"
msgstr ""

#. (itstool) path: example/screen
#: xml/xm_vs_xl_xen2libvirt.xml:103
#, no-wrap
msgid ""
"kernel = \"/boot/vmlinuz-2.6-xenU\"\n"
"  memory = 128\n"
"  name = \"SLE12\"      \n"
"  root = \"/dev/hda1 ro\"\n"
"  disk = [ \"file:/var/xen/sle12.img,hda1,w\" ]"
msgstr ""

#. (itstool) path: example/para
#: xml/xm_vs_xl_xen2libvirt.xml:108
msgid "Convert it to <systemitem class=\"library\">libvirt</systemitem> XML without importing it, and look at its content:"
msgstr ""

#. (itstool) path: example/screen
#: xml/xm_vs_xl_xen2libvirt.xml:112
#, no-wrap
msgid ""
"# xen2libvirt -f xm -c /etc/xen/sle12.xm &gt; /etc/libvirt/qemu/sles12.xml\n"
"  # cat /etc/libvirt/qemu/sles12.xml\n"
"  &lt;domain type='xen'&gt;\n"
"  &lt;name&gt;SLE12&lt;/name&gt;\n"
"  &lt;uuid&gt;43e1863c-8116-469c-a253-83d8be09aa1d&lt;/uuid&gt;\n"
"  &lt;memory unit='KiB'&gt;131072&lt;/memory&gt;\n"
"  &lt;currentMemory unit='KiB'&gt;131072&lt;/currentMemory&gt;\n"
"  &lt;vcpu placement='static'&gt;1&lt;/vcpu&gt;\n"
"  &lt;os&gt;\n"
"  &lt;type arch='x86_64' machine='xenpv'&gt;linux&lt;/type&gt;\n"
"  &lt;kernel&gt;/boot/vmlinuz-2.6-xenU&lt;/kernel&gt;\n"
"  &lt;/os&gt;\n"
"  &lt;clock offset='utc' adjustment='reset'/&gt;\n"
"  &lt;on_poweroff&gt;destroy&lt;/on_poweroff&gt;\n"
"  &lt;on_reboot&gt;restart&lt;/on_reboot&gt;\n"
"  &lt;on_crash&gt;restart&lt;/on_crash&gt;\n"
"  &lt;devices&gt;\n"
"  &lt;disk type='file' device='disk'&gt;\n"
"  &lt;driver name='file'/&gt;\n"
"  &lt;source file='/var/xen/sle12.img'/&gt;\n"
"  &lt;target dev='hda1' bus='xen'/&gt;\n"
"  &lt;/disk&gt;\n"
"  &lt;console type='pty'&gt;\n"
"  &lt;target type='xen' port='0'/&gt;\n"
"  &lt;/console&gt;\n"
"  &lt;/devices&gt;\n"
"  &lt;/domain&gt;"
msgstr ""

#. (itstool) path: example/para
#: xml/xm_vs_xl_xen2libvirt.xml:139
msgid "To import the domain into <systemitem class=\"library\">libvirt</systemitem>, you can either run the same <command>xen2libvirt</command> command without the <option>-c</option> option, or use the exported file <filename>/etc/libvirt/qemu/sles12.xml</filename> and define a new Xen domain using <command>virsh</command>:"
msgstr ""

#. (itstool) path: example/screen
